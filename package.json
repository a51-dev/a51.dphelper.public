{
  "name": "dphelper",
  "version": "0.2.69",
  "description": "Developer Tools",
  "deprecated": false,
  "appCode": "dphelper",
  "appCodeRelease": "dphelper",
  "appName": "dphelper",
  "appType": "beta",
  "appFolder": "/",
  "appAPI": "api",
  "apiDev": "",
  "apiProd": "",
  "main": "index.js",
  "targets": {
    "main": {
      "includeNodeModules": {
        "react": false,
        "react-dom": false
      },
      "isLibrary": true
    }
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "make-dist": "webpack --progress --color --mode production --config webpack.config.js",
    "git": "git add . && git commit -m \"(fix|patch)\" && git pull && git push --all",
    "git-tags": "git push --tags",
    "backup": "backup.bat",
    "goLive": "cli-confirm \"Do you want go live?\" && npm version patch --force && webpack --progress --color --mode production --config webpack.config.js && cd dist && npm publish"
  },
  "eslintConfig": {
    "globals": {
      "window": true
    }
  },
