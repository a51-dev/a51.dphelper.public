  },
  "bugs": {
    "url": "https://github.com/passariello/dphelper/issues"
  },
  "scripts": {
    "make-dist": "webpack --progress --color --mode production --config webpack.config.js",
    "watch": "webpack --watch --progress --color --mode production --config webpack.config.js",
    "goLive": "cd mcp/javascripts && node goLive",
    "publishMe": "cd mcp/batch && publish.bat",
    "optimizeSVG": "cd mcp/javascripts && node optimizeSVG.js",
    "-": "--------------",
    "git": "git add . && git commit -m \"update\" && git pull && git push --all",
    "gitOld": "cd mcp/javascripts && node gitDeploy",
    "--": "-------------",
    "tsc": "tsc -b",
    "lint": "eslint {/**/*,*}.{js,ts,jsx,tsx} --fix",
    "---": "------------",
    "backup": "cd mcp/batch && backup.bat",
    "update": "cd mcp/javascripts && node npmUpdate",
    "": "--------------"
  },
